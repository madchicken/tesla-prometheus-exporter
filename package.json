{
  "name": "tesla-prometheus-exporter",
  "version": "1.0.0",
  "description": "A Prometheus exporter for tesla stats",
  "main": "dist/tesla-prometheus-exporter.js",
  "repository": "git@github.com:madchicken/tesla-prometheus-exporter.git",
  "author": "Pierpaolo Follia <pfollia@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "yarn clean && yarn build:ts",
    "test": "jest --forceExit --coverage --verbose",
    "watch:test": "yarn run test -- --watchAll",
    "build:ts": "tsc",
    "lint": "tsc --noEmit && eslint \"src/*.{js,ts}\" --quiet --fix",
    "prettier": "prettier --single-quote --trailing-comma es5 --write \"{src,__{tests,mocks}__}/**/*.ts\"",
    "clean": "rimraf dist/",
    "serve": "ts-node src/tesla-prometheus-exporter.ts",
    "watch": "concurrently -k -p \"[{name}]\" \"yarn watch:ts\" \"yarn watch:node\"",
    "watch:ts": "tsc -w",
    "watch:node": "ts-nodemon --exec \"yarn serve\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "engines": {
    "node": ">=12.13.0"
  },
  "dependencies": {
    "yargs": "^16.2.0",
    "teslats": "^1.0.3",
    "prom-client": "^12.0.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@types/node": "^11.15.3",
    "@types/express": "^4.17.9",
    "@types/yargs": "^15.0.12",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "http-proxy-middleware": "^1.0.6",
    "husky": "^4.2.3",
    "jest": "^25.5.4",
    "nock": "^12.0.2",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3",
    "ts-jest": "^25.2.1",
    "ts-node": "^9.1.1",
    "@trongnd/ts-nodemon": "^1.0.3",
    "concurrently": "^5.3.0"
  }
}
